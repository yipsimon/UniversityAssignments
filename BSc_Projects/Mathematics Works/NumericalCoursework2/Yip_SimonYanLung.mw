<Group labelreference="L12" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">for j from 1 to numelems(velo) do   <Font foreground="[255,0,0]"># This for loop will loop through velocity list of data</Font>
<Font foreground="[255,0,0]"># If the current velocity value is negative and the next velocity value is positive, then we have found the points that between the value zero.</Font>
    if (evalf(velo[j][2]) &lt; 0 and evalf(velo[j+1][2]) &gt; 0) then  
          x1:=velo[j][1];          <Font foreground="[255,0,0]"># Storing the time value of current iteration</Font>
          x2:=velo[j+1][1];        <Font foreground="[255,0,0]"># Storing the next time value</Font>
          y1:=velo[j][2];          <Font foreground="[255,0,0]"># Storing the velocity value of current iteration</Font>
          y2:=velo[j+1][2];        <Font foreground="[255,0,0]"># Storing the next velocity value</Font>
          <Font foreground="[255,0,0]"># Using the value we have obtained, we estimate the time when the velocity is at zero using linear interpolation. </Font>
          zeropoint := x1 - ((y1*(x2-x1))/(y2-y1)); 
          period := 2*zeropoint;  <Font foreground="[255,0,0]"># Multiple the result by 2 to obtain the period. </Font>
          print(period);         <Font foreground="[255,0,0]"># Print the period of oscillation of the pendulum on screen</Font>
          break;                 <Font foreground="[255,0,0]"># Stop the for loop</Font>
    end if:                      <Font foreground="[255,0,0]"># End of if statement</Font>
end do:                          <Font foreground="[255,0,0]"># End of for loop</Font> </Text-field>
</Input>
</Group>
